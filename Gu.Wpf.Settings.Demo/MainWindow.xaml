<Window x:Class="Gu.Wpf.Settings.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:demo="clr-namespace:Gu.Wpf.Settings.Demo"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:settings="http://gu.se/Settings"
        Title="MainWindow"
        Width="525"
        Height="350"
        d:DataContext="{d:DesignInstance demo:MainWindowViewModel}"
        mc:Ignorable="d">
    <Grid>
        <ItemsControl>
            <settings:SettingControl Header="DoubleMinMax" Setting="{Binding Setting.DoubleMinMax}" />
        </ItemsControl>
    </Grid>
</Window>
